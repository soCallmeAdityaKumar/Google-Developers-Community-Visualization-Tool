<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.Calendar.CalendarFragment">
    <com.events.calendar.views.EventsCalendar
        android:id="@+id/eventsCalendar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/AppbackgroundColor"
        android:overScrollMode="never"
        app:eventDotColor="#F9C23B"
        app:isBoldTextOnSelectionEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:monthTitleColor="@color/MainHeadingText"
        app:primaryTextColor="@color/MainHeadingText"
        app:rangeSelectionColor="#F9C23B"
        app:rangeSelectionEndColor="#F9C23B"
        app:rangeSelectionStartColor="#F9C23B"
        app:secondaryTextColor="@color/MainHeadingText"
        app:selectedTextColor="@color/MainHeadingText"
        app:selectionColor="#4361ee"
        app:weekHeaderColor="#4361ee" />
</androidx.constraintlayout.widget.ConstraintLayout>